{
    "name": "exs/terminal-bundle",
    "type": "symfony-bundle",
    "description": "Terminal command logging bundle for Symfony2",
    "keywords": ["error", "logging", "bundle", "exception", "command", "lock", "crontab", "concurrent"],
    "homepage": "https://github.com/ExSituMarketing/EXSTerminalBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Charles Weiss",
            "email": "charles@ex-situ.com"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "symfony/symfony": "~2.7",
        "symfony/framework-bundle": "~2.7",
        "doctrine/orm": "~2.4",
        "swiftmailer/swiftmailer": "~5.4"
    },
    "require-dev": {
        "exs/error-bundle": "dev-master"
    },
    "autoload": {
        "psr-0": { "EXS\\TerminalBundle": "" }
    },
    "bin": ["bin/runner"],
    "minimum-stability": "dev",
    "target-dir": "EXS/TerminalBundle"
}
